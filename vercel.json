{
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "commands": {
      "install": "npm install --legacy-peer-deps"
    }
  }
}
